package pageobjects;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.How;

import driver.BaseDriver;

public class Tele2ReferralRequestpagePO extends BaseDriver
{
	
	@FindBy(how=How.XPATH,using="//img[@alt='Tele2']")
	public static WebElement Tele2Logo;
	
	@FindBy(how=How.XPATH,using="//a[@href='https://1010.m2m.com']")
	public static WebElement selectKit;
	
	@FindBy(how=How.XPATH,using="//a[text()='M2M.com']")
	public static WebElement M2MDotCom;
	

	
	
}
