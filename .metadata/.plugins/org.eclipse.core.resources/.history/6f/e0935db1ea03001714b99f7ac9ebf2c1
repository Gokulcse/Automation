package pages;

import java.util.Properties;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.ie.InternetExplorerDriver;
import org.openqa.selenium.support.PageFactory;

import utilitymethods.UtilityMethods;
import driver.BaseDriver;
import pageobjects.BellReferralRequestPO;
import pageobjects.BellYourInfoPO;

public class BellYourInfo 
{
	public static FirefoxDriver driver=BaseDriver.driver;
	public static Properties allInputValue = BellHomePage.allInputValue;
	//@BeforeTest
	public static void BrowserIntilation() throws Exception
	{
		allInputValue = UtilityMethods.getBellPropValues();
		driver = BellHomePage.start();
		driver.navigate().refresh();
	}
	public static void referralRequestDataValidation()
	{
		UtilityMethods.StringValidation(BellYourInfoPO.FirstNameInput.getText(), allInputValue.getProperty("FirstName"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.LastNameInput.getText(), allInputValue.getProperty("LastName"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.CompanyNameInput.getText(), allInputValue.getProperty("CompanyName"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.PositionRoleInput.getText(), allInputValue.getProperty("Position"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.PhoneNumberInput.getText(), allInputValue.getProperty("PhoneNumber"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.EmailInput.getText(), allInputValue.getProperty("E-mailAddress"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.ModuleInput.getText(), allInputValue.getProperty("Module"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.BusinessInput.getText(), allInputValue.getProperty("BusinessType"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.SimNanolabel.getText(), allInputValue.getProperty("SimType"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.VolumetoDeployInput.getText(), allInputValue.getProperty("VolumeToDeploy"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.IN12MonthLabel.getText(), allInputValue.getProperty("TimeFrame"), "equalsignorecase");
		UtilityMethods.StringValidation(BellYourInfoPO.Description.getText(), allInputValue.getProperty("Description"), "equalsignorecase");
	}
	public static void TermsOfServiceTextValidation()
	{
		UtilityMethods.DisplayEnableValidator(BellYourInfoPO.TermsStatusText, "NotEqual","Terms Of Services");
		UtilityMethods.StringValidation(BellYourInfoPO.TermsStatusText.getText(), allInputValue.getProperty("TermsOfService"), "equalsignorecase");
		UtilityMethods.RequiredFieldValidation(BellYourInfoPO.TermsStatusText, BellYourInfoPO.TermsOfServicesError,"TextBox");	
	}
	
	public static void TermsOfServicePopUpCloseIcon()
	{
		BellYourInfoPO.TermsOfServicesLink.click();
		UtilityMethods.DisplayEnableValidator(BellYourInfoPO.CloseIcon, "NotEqual","Terms Of Services Icon");
		BellYourInfoPO.CloseIcon.click();
			
	}
	
	public static void TermsOfServicePopUpCloseButton()
	{
		BellYourInfoPO.TermsOfServicesLink.click();
		UtilityMethods.DisplayEnableValidator(BellYourInfoPO.CloseButton, "NotEqual","Terms Of Services Button");
		BellYourInfoPO.CloseButton.click();
	}
	
	public static void TermsOfServicePDFDownload()
	{
		BellYourInfoPO.TermsOfServicesLink.click();
		UtilityMethods.DisplayEnableValidator(BellYourInfoPO.TermsOfServicesDownload, "NotEqual","Terms Of Services Download");
		BellYourInfoPO.TermsOfServicesDownload.click();
		WebElement webElement = driver.findElement(By.xpath(""));//You can use xpath, ID or name whatever you like
		webElement.sendKeys(Keys.ENTER);
	}
	
	public static void validateOperatorLogo()
	{
		PageFactory.initElements(driver, BellYourInfoPO.class);
		UtilityMethods.DisplayEnableValidator(BellYourInfoPO.BellLogo, "NotEqual","Bell Operator Logo in IoT Starter Kit");	
		UtilityMethods.Imagevalidation(BellYourInfoPO.BellLogo,"src",allInputValue.getProperty("bellLogo"),"Bell Operator Logo");		
		
	} 
	
	//@Test(priority=2)
	public static void validateCompanyLogo()
	{
		PageFactory.initElements(driver, BellYourInfoPO.class);
		UtilityMethods.DisplayEnableValidator(BellYourInfoPO.CiscoLogo, "NotEqual","Cisco Jasper Company Logo in IoT Starter Kit");		
		UtilityMethods.Imagevalidation(BellYourInfoPO.CiscoLogo,"src",allInputValue.getProperty("ciscoLogo"),"Cisco Jasper Company Logo");		
		
	}
 
	//@Test(priority=3)
	public static void ReferralRequestTextValidation()
	{
		PageFactory.initElements(driver, BellYourInfoPO.class);
		UtilityMethods.StringValidation(BellYourInfoPO.ContactInfoText.getText(), "Contact Information", "equalsignorecase");
